	Publisher = {John Benjamins Publishing Company},
	Title = {The Logophoric Hierarchy and Variation in Dogon},
	Year = {2002},
	Bdsk-File-1 = {}}

@article{Culy2001,
	Abstract = {Donno S\`o, a Dogon language of Mali, has a class of verbs (C3) that exhibits an interesting set of formal & semantic properties. The verbs in this class have different derivational histories; they also have various types of meaning (middle, middle - related, non - middle). Although C3 verbs cannot be unified derivationally or semantically, they can all be defined both paradigmatically & in terms of phonotactic constraints, like the other two verb classes in Donno So. Comparison with other Dogon languages shows how the middle evolved in Dogon & how Donno So C3 verbs in turn evolved from the middle. These results expand Kemmer's (1993) discussion of the processes involved in the evolution of middle systems. The comparison also provides some hypotheses about the history of Dogon. 24 References. Adapted from the source document},
	Author = {Culy, Christopher and Fagan, Sarah M B},
	Date-Modified = {2014-10-30 14:14:12 +0000},
	Journal = {Studies in African Linguistics},
	Number = {2},
	Pages = {171--194},
	Title = {The History of the Middle in Dogon},
	Volume = {30},
