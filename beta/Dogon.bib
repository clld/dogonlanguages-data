@article{Culy1997,
	Abstract = {It is shown that logophoricity & point of view are distinct phenomena by examining morphologically distinct logophoric pronouns, showing that they are primarily indirect discourse forms & usually do not represent point of view at all. In contrast to indirect reflexives in English, Icelandic, Italian, Japanese, & Latin, some African languages (eg, Dogon, Ewe, Tupuri) contain morphologically distinct logophoric pronouns that cannot be defined in terms of binding conditions, but are simply a type of indirect discourse form & only represent point of view when they are not licensed by a predicate. It is demonstrated that these pronouns are similar to indirect reflexives in that (1) when the logophoric pronoun is obligatory, it does not represent point of view, (2) when a logophoric pronoun indicates the point of view of the internal protagonist, the use of a personal pronoun indicates a point of view other than that of the internal protagonist, & (3) some logophoric pronouns & indirect reflexives are always obligatory, & thus never represent point of view. 33 References. Adapted from the source document},
	Author = {Culy, Christopher},
	Date-Modified = {2014-10-30 14:14:04 +0000},
	Journal = {Linguistics},
	Number = {5},
	Pages = {845--859},
	Title = {Logophoric Pronouns and Point of View},
	Volume = {35},
	Year = {1997}}

@incollection{Culy2002,
	Abstract = {The presence of logophoric hierarchy in discourse produced by speakers for whom Donno S(open o), a Dogon (Niger Congo, Gur family) dialect, is a first or second language, or who have only mastered its passive knowledge, is investigated with references to earlier studies (Culy, 1994 & Culy et al, 1994) on the implicational hierarchy of predicates licensing logophoric pronouns. Evidence is found for the strong effect of logophoric hierarchy in this language, both within & across varieties. It is observed that logophoric hierarchy is not a logical necessity, but only a contingent fact about human language. 3 References. Z. Dubiel},
	Address = {Amsterdam},
	Author = {Culy, Christopher},
	Booktitle = {Reported Discourse: A meeting ground for different linguistic domains},
	Date-Modified = {2014-10-30 14:10:32 +0000},
